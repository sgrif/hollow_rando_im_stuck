#!/bin/bash

if [ -z "$(git status --porcelain)" ]; then
  git checkout gh-pages
  git reset --hard main
  (cd wasm && wasm-pack build --release)
  (cd www && rm -rf dist && npm run build)
  mv www/dist/* .
  git add .
  git commit -m "npm build"
  git push -f
  git checkout main
else
  echo "Uncommitted changes"
fi
